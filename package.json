    "preview": "vite preview"
  },
  "devDependencies": {
    "@biomejs/biome": "1.3.3",
    "@types/lscache": "^1.3.4",
    "atcoder-problems-api": "github:key-moon/atcoder-problems-api",
    "lscache": "^1.3.2",
    "sass": "^1.69.6",
    "tsc-silent": "^1.2.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-monkey": "^3.4.0"
  }
}
